{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@/shared": ["../shared/models"],
    }
  },
  "include": ["**/*"],
  "exclude": ["node_modules", "dist"],
}
